{
"name": "Swear Jar",
"version": "1.0",
"description": "Don't swear",
"permissions": ["activeTab", "declarativeContent", "storage", "webRequest", "*://*/*", "notifications", "http://*/*", "https://*/*", "webRequestBlocking", "tabs"],
"options_page": "options.html",
"background": {
  "scripts": ["background.js", "notif.js"],
  "persistent": true
},
"content_security_policy": "script-src 'self' https://www.paypal.com https://www.paypalobjects.com 'sha256-LKKg1Vh3S9JSrCl7wK2GqaUPxWT1gn4Fu/CAUdVYJIU=' 'sha256-QxDd7xFukk2bf+v4KTMQ7sGwi2ic2sczh7zupXZAZQg='; object-src 'self'",
"page_action": {
  "default_popup": "popup.html",
  "default_icon": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
},
"icons": {
  "16": "images/get_started16.png",
  "32": "images/get_started32.png",
  "48": "images/get_started48.png",
  "128": "images/get_started128.png"
},

"web_accessible_resources": [
   "swearingisbad.html"
],
"manifest_version": 3
}
